{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "main": "dist/src/main.js",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16 <18"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "cross-env NODE_ENV=development nest start",
    "start:prod": "cross-env NODE_ENV=production nest start",
    "start:stage": "cross-env NODE_ENV=stage nest start",
    "start:stage-v2": "cross-env NODE_ENV=stage-v2 nest start",
    "start:local": "cross-env NODE_ENV=local nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "npm run build:dev && cross-env NODE_ENV=local npx typeorm -d dist/src/db/data-source.js ",
    "migration:generate": "npm run typeorm -- migration:generate ./src/db/migrations/migrate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "typeorm:dev": "npm run build:local && cross-env NODE_ENV=development npx typeorm -d dist/src/db/data-source.js ",
    "migration:generate:dev": "npm run typeorm -- migration:generate ./src/db/migrations/migrate",
    "migration:run:dev": "npm run typeorm -- migration:run",
    "migration:revert:dev": "npm run typeorm -- migration:revert",
    "typeorm:stg": "npm run build:stage && cross-env NODE_ENV=stage npx typeorm -d dist/src/db/data-source.js ",
    "migration:generate:stg": "npm run typeorm -- migration:generate ./src/db/migrations/migrate",
    "migration:run:stg": "npm run typeorm -- migration:run",
    "migration:revert:stg": "npm run typeorm -- migration:revert",
    "seed:config": "cross-env NODE_ENV=local ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run:local": "cross-env NODE_ENV=local nest start --entryFile seed",
    "seed:run:dev": "cross-env NODE_ENV=development nest start --entryFile seed",
    "seed:run:prod": "cross-env NODE_ENV=production nest start --entryFile seed",
    "seed:run:stage": "cross-env NODE_ENV=stage nest start --entryFile seed",
    "seed:run:stage-v2": "cross-env NODE_ENV=stage-v2 nest start --entryFile seed",
    "seed:run:stage-v3": "cross-env NODE_ENV=stage-v3 nest start --entryFile seed",
    "build:local": "cross-env NODE_ENV=local nest build",
    "build:dev": "cross-env NODE_ENV=development nest build",
    "build:prod": "cross-env NODE_ENV=production nest build",
    "build:stage": "cross-env NODE_ENV=stage nest build",
    "build:stage-v2": "cross-env NODE_ENV=stage-v2 nest build",
    "build:stage-v3": "cross-env NODE_ENV=stage-v2 nest build",
    "pm2_start_dev": "pm2 start ecosystem.config.js --env development",
    "pm2_start_stg": "pm2 start ecosystem.config.js --env stage",
    "pm2_start_prod": "pm2 start ecosystem.config.js --env production",
    "pm2_start_stg-v2": "pm2 start ecosystem.config.js --env stage_v2",
    "pm2_start_stg-v3": "pm2 start ecosystem.config.js --env stage_v3",
    "pm2_stop_dev": "pm2 stop e-library-app-development",
    "pm2_stop_stg": "pm2 stop e-library-app-stage",
    "pm2_stop_prod": "pm2 stop e-library-app-production",
    "pm2_stop_stg-v2": "pm2 stop e-library-app-stage-v2",
    "pm2_stop_stg-v3": "pm2 stop e-library-app-stage-v3",
    "pm2_restart_dev": "pm2 restart e-library-app-development",
    "pm2_restart_stg": "pm2 restart e-library-app-stage",
    "pm2_restart_prod": "pm2 restart e-library-app-production",
    "pm2_restart_stg-v2": "pm2 restart e-library-app-stage-v2",
    "pm2_restart_stg-v3": "pm2 restart e-library-app-stage-v3",
    "pm2_delete_dev": "pm2 delete e-library-app-development",
    "pm2_delete_stg": "pm2 delete e-library-app-stage",
    "pm2_delete_prod": "pm2 delete e-library-app-production",
    "pm2_delete_stg-v2": "pm2 delete e-library-app-stage-v2",
    "pm2_delete_stg-v3": "pm2 delete e-library-app-stage-v3",
    "generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n migration -p"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.391.0",
    "@aws-sdk/client-ses": "^3.398.0",
    "@casl/ability": "^6.5.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.2.6",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.2.6",
    "@types/express-session": "^1.17.7",
    "@types/multer": "^1.4.7",
    "@types/passport": "^1.0.12",
    "bcrypt": "^5.1.0",
    "bluebird": "^3.7.2",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express-session": "^1.17.3",
    "fast-csv": "^4.3.6",
    "handlebars": "^4.7.8",
    "nest-access-control": "^2.2.0",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
